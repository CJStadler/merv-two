<%= form_for @user, url: "/authenticate" do |f| %>
    <div>
        <%= f.label :name %>
        <%= f.text_field :name, required: true %>
    </div>
    <div>
        <%= f.label :password %>
        <%= f.password_field :password, required: true %>
    </div>
    <%= f.submit "Log In" %>
<% end %>
